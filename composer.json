{
    "name": "szeidler/fotoweb-php-sdk",
    "description": "Fotoweb by fotoware.com PHP SDK",
    "keywords": ["fotoweb", "fotoware", "dam"],
    "homepage": "https://github.com/szeidler/fotoweb-php-sdk",
    "require": {
        "php": ">=7.4",
        "psr/http-message": "^1.1 || ^2.0",
        "guzzlehttp/guzzle-services": "^1.1",
        "kamermans/guzzle-oauth2-subscriber": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Zeidler",
            "email": "info@stephanzeidler.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fotoweb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fotoweb\\Tests\\": "tests"
        }
    }
}
